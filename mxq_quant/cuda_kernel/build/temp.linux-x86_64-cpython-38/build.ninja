ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /user/jhli/anaconda3/envs/envName/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /user/jhli/anaconda3/envs/envName/include -fPIC -O2 -isystem /user/jhli/anaconda3/envs/envName/include -fPIC -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include/TH -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda/include -I/user/jhli/anaconda3/envs/envName/include/python3.8 -c
post_cflags = -g -O3 -fopenmp -lgomp -std=c++17 -DENABLE_BF16 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=mxq_inference_engine -D_GLIBCXX_USE_CXX11_ABI=0
cuda_cflags = -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include/TH -I/user/jhli/anaconda3/envs/envName/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda/include -I/user/jhli/anaconda3/envs/envName/include/python3.8 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 -std=c++17 -DENABLE_BF16 -U__CUDA_NO_HALF_OPERATORS__ -U__CUDA_NO_HALF_CONVERSIONS__ -U__CUDA_NO_BFLOAT16_OPERATORS__ -U__CUDA_NO_BFLOAT16_CONVERSIONS__ -U__CUDA_NO_BFLOAT162_OPERATORS__ -U__CUDA_NO_BFLOAT162_CONVERSIONS__ --expt-relaxed-constexpr --expt-extended-lambda --use_fast_math --threads=8 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=mxq_inference_engine -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /user/jhli/quantization/temp/MXQ/mxq_quant/cuda_kernel/build/temp.linux-x86_64-cpython-38/csrc/pybind.o: compile /user/jhli/quantization/temp/MXQ/mxq_quant/cuda_kernel/csrc/pybind.cpp
build /user/jhli/quantization/temp/MXQ/mxq_quant/cuda_kernel/build/temp.linux-x86_64-cpython-38/csrc/quantization/gemv_cuda.o: cuda_compile /user/jhli/quantization/temp/MXQ/mxq_quant/cuda_kernel/csrc/quantization/gemv_cuda.cu
build /user/jhli/quantization/temp/MXQ/mxq_quant/cuda_kernel/build/temp.linux-x86_64-cpython-38/csrc/quantization/gemv_mxq_cuda.o: cuda_compile /user/jhli/quantization/temp/MXQ/mxq_quant/cuda_kernel/csrc/quantization/gemv_mxq_cuda.cu







